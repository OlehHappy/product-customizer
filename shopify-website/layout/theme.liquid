<body id="{{ page_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }} {{ settings.theme_float_style }} {{ settings.theme_layout_mode }}" >
    <div id="page">
        <header id="header" role="banner">
            {% include 'header' %}
        </header>
        {% if settings.slider_home_enable and template == 'index' %}
            <section id="slideshow" class="clearfix">
                {% include 'slideshow' %}
            </section>
        {% endif %}
        <main class="main-content" role="main">
            {{ content_for_layout }}
        </main>
        <footer id="footer" role="contentinfo">
            {% include 'footer' %}
        </footer>
      	{% if settings.theme_quickview_enable %}
            {% include 'product-quickview' %}
        {% endif %}
        {% include 'footer-js' %}
    </div>
    <p id="back-top">
        <a href="#top" title="Scroll To Top">Scroll To Top</a>
    </p>
    {% if settings.theme_paneltool_enable %}
        {% include 'paneltool' %}
    {% endif %}
    {% if settings.apollo_megamenu_enable %}
        {% include 'block-menu-canvas' %}
    {% endif %}
