@font-face {
    font-family: 'ralewayheavy';
    src: url('{{ "raleway.heavy-webfont.eot" | asset_url }}');
    src: url('{{ "raleway.heavy-webfont.eot?#iefix" | asset_url }}') format('embedded-opentype'),
         url('{{ "raleway.heavy-webfont.woff2" | asset_url }}') format('woff2'),
         url('{{ "raleway.heavy-webfont.woff" | asset_url }}') format('woff'),
         url('{{ "raleway.heavy-webfont.ttf" | asset_url }}') format('truetype'),
         url('{{ "raleway.heavy-webfont.svg#ralewayheavy" | asset_url }}') format('svg');
    font-weight: normal;
    font-style: normal;

}

